# Analytics Configuration
# Copy this file to .env for local development

# Plausible Analytics
# Domain registered in your Plausible account
# Default: hasintha01.github.io
PUBLIC_PLAUSIBLE_DOMAIN=hasintha01.github.io

# Google Analytics 4 (Optional)
# Get your Measurement ID from analytics.google.com
# Format: G-XXXXXXXXXX
# PUBLIC_GA4_ID=G-XXXXXXXXXX

# Enable/Disable Analytics
# Set to false for local development if needed
PUBLIC_ANALYTICS_ENABLED=true
